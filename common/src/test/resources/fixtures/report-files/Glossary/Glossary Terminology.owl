    <cts:cdiscSubmissionValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >European Medicines Agency (EMA)</cts:cdiscSubmissionValue>
  </mms:PermissibleValue>
  <mms:PermissibleValue rdf:ID="C67497.C52095">
    <mms:inValueDomain rdf:resource="#C67497"/>
    <cts:nciPreferredTerm rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Metadata</cts:nciPreferredTerm>
    <cts:nciCode rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >C52095</cts:nciCode>
    <cts:cdiscDefinition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
