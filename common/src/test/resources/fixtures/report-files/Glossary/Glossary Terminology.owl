  <mms:PermissibleValue rdf:ID="C67497.C142544">
    <mms:inValueDomain rdf:resource="#C67497"/>
    <cts:nciPreferredTerm rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >European Union Competent Authority</cts:nciPreferredTerm>
    <cts:nciCode rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >C142544</cts:nciCode>
    <cts:cdiscDefinition rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The regulatory body charged with monitoring compliance with the national statutes and regulations of European Member States.</cts:cdiscDefinition>
    <cts:cdiscSubmissionValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Competent Authority (CA)</cts:cdiscSubmissionValue>
  </mms:PermissibleValue>
  <mms:PermissibleValue rdf:ID="C67497.C142543">
